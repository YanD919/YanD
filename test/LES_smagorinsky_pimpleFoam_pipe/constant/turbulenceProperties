FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType      LES;

RAS
{
    RASModel        kOmegaSST;

    turbulence      on;

    printCoeffs     on;

    //alphaK1         500;
}

LES
{
    LESModel        Smagorinsky;

    turbulence      on;

    printCoeffs     on;

    //alphaK1         500;

    delta           cubeRootVol;
    SmagorinskyCoeffs
    {
        Ck                  0.094;
        Ce                  1.048;
    }
}
