FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 5.39e-7;

boundaryField
{
    "(inlethemisphere|outlethemisphere|tubing)"
    {
        type            calculated;
        value           $internalField;
    }

    inlet
    {
        type            calculated;
	value           $internalField;
    }

    outlet
    {
        type            calculated;
	value           $internalField;
    }
}


